{
  "name": "bootstrap-content-templates",
  "description": "An extension for Squiz Matrix to enable Bootstrap based Content Templates.",
  "version": "0.1.0",
  "homepage": "https://matrix.squiz.net/",
  "author": {
    "name": "Bart Banda",
